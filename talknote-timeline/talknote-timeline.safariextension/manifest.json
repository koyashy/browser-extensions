{
    "manifest_version": 2,

    "name": "Talknote timeline extension",
    "description": "This extension shows timeline in news page.",
    "version": "1.0.2",
    "update_url": "https://s3-ap-northeast-1.amazonaws.com/mediba-browser-extensions/talknote-timeline/updates.xml",

    "icons": {
        "16": "icons/talknote_logo_16.png",
        "48": "icons/talknote_logo_48.png",
        "128": "icons/talknote_logo_128.png"
    },

    "content_scripts": [{
        "matches": ["https://company.talknote.com/*/news/"],
        "css": ["css/style.css"],
        "js": [
            "js/jquery-2.2.1.min.js",
            "js/script.js"
        ],
        "run_at": "document_end"
    }]
}
